{
  "name": "lutabook",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "sonar": "npm run test && node server/sonar-project.js && cd client && npm run sonar ",
    "test": "jest --coverage --detectOpenHandles --setupFiles dotenv/config && cd client && npm run test",
    "start": "node server/src/index.js",
    "dev": "nodemon server/src/index.js",
    "build": "cd client && npm install && npm run build"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "server/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "client"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "client"
    ],
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "jestSonar": {
    "reportPath": "server/coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "engines": {
    "node": "14.17.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.0",
    "jest": "26.6.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2",
    "sonarqube-scanner": "^2.8.1",
    "supertest": "^6.1.6"
  }
}
